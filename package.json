{
  "name": "backend-test",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean:dist": "rimraf ./dist",
    "clean:modules": "rimraf ./node_modules",
    "clean": "npm run clean:dist && npm run clean:modules",
    "lint": "npx eslint src/**/*.ts",
    "lint:fix": "npx eslint --fix src/**/*.ts && npx eslint --fix test/**/*.ts",
    "prebuild": "npm run clean:dist",
    "build": "tsc",
    "create:db": "ts-node src/db/create-db.ts",
    "seed:db": "ts-node src/db/seed-db.ts",
    "drop:db": "ts-node src/db/drop-db-tables.ts",
    "test:db": "ts-node src/db/test-db.ts",
    "start": "node dist/src/index",
    "start:dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "pretest": "npm run build",
    "test": "mocha dist/test/**/*.js",
    "test:dev": "ts-mocha test/**/*.spec.ts -w --watch-files '**/*.ts'"
  },
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/express": "^5.0.0",
    "@types/lodash": "^4.14.188",
    "@types/mocha": "^10.0.0",
    "@types/node": "^18.11.9",
    "@types/pg": "^8.11.10",
    "@types/sinon": "^10.0.13",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "chai": "^4.3.7",
    "eslint": "^8.27.0",
    "mocha": "^10.1.0",
    "rimraf": "^3.0.2",
    "sinon": "^14.0.2",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.4",
    "zod": "^3.23.8"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "pg": "^8.13.0"
  }
}
